<h1>Welcome to SlackerNews!</h1>

<%= link_to 'New submission', new_submission_path %>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @submissions.each do |submission| %>
    <tr>
      <td>
        <a href = <%= submission.link %>>
          <%= submission.title %></td>
        </a>
      <td><%= submission.text %></td>
    </tr>
  <% end %>
</table>